<div class="card cardIndex">
  <img src="<%= `/images/${post.image}` %>">
  <div class="card-body cardBody">
    <h5 class="card-title">
      <%= post.title %>
    </h5>
    <p class="card-text cardBody">
      <%= post.description %>
    </p>
    <a href="/posts/<%=post.id%>"">Ler mais</a>
  </div>
</div>